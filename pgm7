import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String firstName = sc.next();
        String lastName  = sc.next();
        int n = sc.nextInt();

        String fullName = firstName + lastName;

        for (int i = 1; i <= n; i++) {
            System.out.println(fullName);
        }
    }
}
